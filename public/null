  'user_id' => '01k594mtn4b162cyq6a3tpnf3p',
  'grade_id' => '01k594mtgxcq0eqhejcmcqx9rh',
  'subject_id' => '01k594mtrah8zp9kf2p6dhrc25',
  'month' => '9',
];

$journals = Journal::query()
  ->where('user_id', $journals['user_id'])
  ->where('academic_year_id', $journals['academic_year_id'])
  ->where('grade_id', $journals['grade_id'])
  ->where('subject_id', $journals['subject_id'])
  ->whereMonth('date', $journals['month'])
  ->orderBy('date', 'asc')
  ->first()->getMedia('activity_photos');
