$journals = Journal::query()
            ->where('user_id', $journals['user_id'])
            ->where('academic_year_id', $this->journals['academic_year_id'])
            ->where('grade_id', $journals['grade_id'])
            ->where('subject_id', $journals['subject_id'])
            ->when($this->journals['month'] !== null, function (Builder $query) {
                $query->whereMonth('date', $journals['month']);
            })
            ->orderBy('date', 'asc')
            ->get();
